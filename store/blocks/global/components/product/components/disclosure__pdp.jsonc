{
    "disclosure-layout-group#pdp__secciones": {
        "title": "disclosure Menú ",
        "children": [
            "disclosure-layout#descripcion",
            "disclosure-layout#cuidados",
            "disclosure-layout#detalles"
        ]
    },
    "disclosure-layout#descripcion": {
        "title": "Disclosure descripción",
        "children": [
            "disclosure-trigger#descripcion",
            "disclosure-content#descripcion"
        ]
    },
    "disclosure-trigger#descripcion": {
        "title": "Menú descripción",
        "children": [
            "rich-text#descripcion",
            "disclosure-state-indicator#descripcion"
        ],
        "props": {
            "blockClass": "contenedorMenuMujer"
        }
    },
    "rich-text#descripcion": {
        "title": "Descripción",
        "props": {
            "text": "Descripción",
            "font": "heading-2",
            "blockClass": "title-disclosure"
        }
    },
    "disclosure-state-indicator#descripcion": {
        "title": "Iconos__mas--menos",
        "props": {
            "Show": "image#down__descrip",
            "Hide": "image#up__descrip"
        }
    },
    "image#up__descrip": {
        "title": "Icono mas",
        "props": {
            "src": "assets/img/icons/menos.png",
            "maxWidth": 30,
            "maxHeight": 30
        }
    },
    "image#down__descrip": {
        "title": "icono menos",
        "props": {
            "src": "assets/img/icons/mas.png",
            "maxWidth": 30,
            "maxHeight": 30
        }
    },
    "disclosure-content#descripcion": {
        "title": "Contenido Descripción",
        "children": [
            "disclosure-layout-group#descripcion"
        ]
    },
    "disclosure-layout-group#descripcion": {
        "title": "Información de descripción",
        "children": [
            "product-description"
        ]
    },



    "disclosure-layout#cuidados": {

        "children": [
            "disclosure-trigger#cuidados",
            "disclosure-content#cuidados"
        ]
    },
    "disclosure-trigger#cuidados": {
        "title": "Menú cuidados",
        "children": [
            "rich-text#cuidados",
            "disclosure-state-indicator#__cuidados"
        ],
        "props": {
            "blockClass": "contenedorMenuMujer"
        }
