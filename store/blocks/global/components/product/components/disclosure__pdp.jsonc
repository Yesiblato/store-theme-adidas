        "props": {
            "Show": "image#down__cuidados",
            "Hide": "image#up__cuidados"
        }
    },
    "image#up__cuidados": {
        "props": {
            "src": "assets/img/icons/menos.png",
            "maxWidth": 30,
            "maxHeight": 30
        }
    },
    "image#down__cuidados": {
        "props": {
            "src": "assets/img/icons/mas.png",
            "maxWidth": 30,
            "maxHeight": 30
        }
    },
