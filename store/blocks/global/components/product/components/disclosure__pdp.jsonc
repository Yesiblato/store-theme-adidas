        "children": [
            "disclosure-layout-group#cuidados"
        ]
    },
    "disclosure-layout-group#cuidados": {
        "children": [
            "flex-layout.row#contenedor__cuidados"
        ],
        "props": {
            "blockClass": "productCuidadosContainer"
        }
    },
    "flex-layout.row#contenedor__cuidados": {
        "title": "Contenedor__ cuidados",
        "children": [
            "flex-layout.col#contenedor__cuidados1",
            "flex-layout.col#contenedor__cuidados2"
        ],
        "props": {
            "blockClass": "productCuidadosContainer"
        }
    },
    "flex-layout.col#contenedor__cuidados1": {
        "title": "Contenedor cuidados 1",
        "children": [
            "flex-layout.row#contenedor__cuidados1"
        ]
    },
    "flex-layout.row#contenedor__cuidados1": {
        "title": "Contenedor cuidados 1 row",
        "children": [
           "flex-layout.col#contenedor__cuidados1--secciones"

        ]
    },
    "flex-layout.col#contenedor__cuidados1--secciones": {
        "title": "Contenedor cuidados secciones",
        "children": [
            "rich-text#titulo1",
            "flex-layout.row#contenedor__cuidados1--secciones"
        ]
    },
    "rich-text#titulo1": {
        "title": "Título 1",
        "props": {
            "text": "INSTRUCCIONES DE LAVADO",
            "blockClass": "titulo__disclosure--pdp"
        }
    },
    "flex-layout.row#contenedor__cuidados1--secciones": {
        "title": "Contenedor cuidados secciones row",
        "children": [
            "flex-layout.col#contenedor__cuidados1--seccion1",
            "flex-layout.col#contenedor__cuidados1--seccion2"
        ]
    },
    "flex-layout.col#contenedor__cuidados1--seccion1": {
        "title": "Contenedor cuidados secciones col",
        "children": [
            "flex-layout.row#contenedor__parte1",
            "flex-layout.row#contenedor__parte2",
            "flex-layout.row#contenedor__parte3"

        ]
    },
    "flex-layout.row#contenedor__parte1": {
        "title": "Contenedor parte 1",
        "children": [
            "image#icono__parte1",
            "rich-text#consejo1"
        ],
        "props": {
            "blockClass": "contenedor__parte1",
            "preserveLayoutOnMobile": true,
            "preventHorizontalStretch": true,
            "colGap": 5,
            "paddingBottom": 5
        }
    },
    "image#icono__parte1": {
        "title": "Imagen parte 1",
        "props": {
            "src": "assets/img/icons/blanqueador.png",
            "maxWidth": 24,
            "maxHeight": 24
        }
    },
    "rich-text#consejo1": {
        "title": "Consejo 1",
        "props": {
            "text": "No usar blanqueador"
        }
    },
    "flex-layout.row#contenedor__parte2": {
